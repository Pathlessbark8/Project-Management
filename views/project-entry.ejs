<!DOCTYPE html>
<html>

<head>
  
<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js">  </script>
  <script>
    $(document).ready(function(){
      var proj_id, title, type, desc, pre_req;
      $("#submit").click(function(){
        proj_id=$("#proj_id").val();
        title=$("#title").val();
        type=$("#type").val();
        desc=$("#desc").val();
        pre_req=$("#pre_req").val();
        if(proj_id.length == 0 || title.length==0 || type.length==0||desc.length==0||pre_req.length==0 ){
            alert("Enter all the fields!");
        } 
        else{
             $.post("http://localhost:3000/prof/project-entry",{proj_id: proj_id, title: title, type: type, desc: desc, pre_req: pre_req}).then( function(data){
             // if(data === 'yes') {
             //     alert("data got successfully");
             //     }
             window.location = "prof/project-list"
            });
        }
      });
    });
  </script>
<style>
  @import url('https://fonts.googleapis.com/css2?family=Varela+Round&display=swap');

