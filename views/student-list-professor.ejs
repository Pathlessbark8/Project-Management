<!DOCTYPE html>
<html lang="en">
<head>
  <title>Student Applied</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <style>
      body{
       background-color: #FD5D5D;
      }
  
      h1 {
        font-weight: 700;
        text-transform: capitalize;
        font-size: 42px;
        margin: 0px;
        color: #fff;
        text-align: center;
        position: relative;
        margin-bottom: 30px;
      }
      .container-wrapper {
      padding: 5em;
      margin: 2em;
      background-color: #FF8080;
  }

  .table-shift {
        margin-top: 100px;
      }

      .shift-btn{
      margin-right: 100px;
    }
    </style>
</head>
<body>
  <header>
    <!-- Fixed navbar -->
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark py-3 navbar-shift">
      <div class="container-fluid">
        <a class="navbar-brand"  style="font-size:25px; font-weight: bold;">Projects you are offering</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
          <ul class="navbar-nav me-auto mb-2 mb-md-0">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="#"></a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="#"></a>
            </li>
          </ul>
          <form class="d-flex" method="get" >
           <!-- <li class="nav-item">
             <a class="nav-link active" aria-current="page" href="#">Login</a>
           </li>
           <li class="nav-item">
             <a class="nav-link active" aria-current="page" href="#">Sign-up</a>
           </li> -->
            <!-- <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search"> -->
            <!-- <button class="btn btn-outline-info" type="button" style="margin-right: 15px;"  onclick="window.location='/interlogin';">Login</button>
            <div class="space"> </div> -->
            <button class="btn btn-outline-danger shift-btn" type="button" onclick="window.location='/intersignup';">Sign Out</button>
          </form>
        </div>
      </div>
    </nav>
  </header>
    <div class="container mt-3">
<!-- <h2 style="text-align: center;">Student Applied</h2> -->
    <table class="table table-hover container-wrapper table-shift">
        <thead>
            <th>S.No</th>
            <th>Student ID</th>
            <th>Branch</th>
            <th>Name</th>
            <th>CGPA</th>
            <th>E-Mail ID</th>
            <th>Send Email</th>
        </thead>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
          console.log(data)
        var name = data.fname + ' ' + data.lname;
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.sid %></td>
            <td><%=data.branch %></td>
            <td><%=name %></td>
            <td><%=data.cgpa %></td>
            <td><%=data.email %></td>
            <!-- <td><%=data.l_name %></td> -->
            <td><span ><a href="mailto:<%=data.email%>"><button  id="<%=data.sid%>" type="button" class="btn btn-danger" value="Mail" >Mail</button></a></span></td>
            
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>